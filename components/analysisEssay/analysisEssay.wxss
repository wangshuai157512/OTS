.essay {
    padding: 0 50rpx 20rpx 50rpx;
    box-sizing: border-box;
}
.essay-area-content {
    height: 260rpx;
    background:rgba(247,249,252,1);
    border-radius:16rpx;
    box-sizing: border-box;
    padding: 24rpx 20rpx;
    word-wrap:break-word
}

/* 解析部分 */
.analysis-content {
    width: 100%;
    background:rgba(255,255,255,1);
    padding: 20rpx 30rpx 0 30rpx;
    box-sizing: border-box;
    font-size:32rpx;
    position: relative;
    font-family:PingFangSC-Medium,PingFang SC;
    color:rgba(115,134,153,1);
}
.box {
    width: 750rpx;
    height: 20rpx;
    /* background:rgba(247,249,252,1); */
    background:rgba(247,249,252,1);
    position: absolute;
    left: 0;
    top: 0rpx;
}
.analysis-content-nav {
    width: 100%;
    height: 120rpx;
    /* line-height: 120rpx; */
    margin: 20rpx 20rpx 0 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size:34rpx;
    font-weight:500;
    color:rgba(244,74,74,1);
}
.analysis-content-nav-right {
    width:112rpx;
    height:44rpx;
    line-height: 44rpx;
    text-align: center;
    background:rgba(255,255,255,0);
    border-radius:22rpx;
    border:1px solid rgba(187,197,205,1);
    font-size:22rpx;
    color:rgba(187,197,205,1);
}
.analysis-content-question {
    width:100%;
    /* height:180rpx; */
    padding: 47rpx 20rpx;
    box-sizing: border-box;
    font-weight:500;
    color:rgba(39,46,64,1);
    border-top: 1px solid #F2F6FA;
}
.analysis-content-question-all {
    margin-top: 28rpx;
    font-size:30rpx;
    color:rgba(10,192,146,1);
}
.analysis-content-question-r {
    margin-left: 120rpx;
}
.analysis-content-question-err {
    color:rgba(244,74,74,1);
}
.analysis-content-question-content {
    margin-top: 28rpx;
    font-size:28rpx;
    color:rgba(115,134,153,1);
    line-height: 1.5;
    letter-spacing: 4rpx;
    font-weight: 400;
}
.score {
    font-size:34rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(10,192,146,1);
}
.parserA view {
    line-height: 1.5!important;
}
