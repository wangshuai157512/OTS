.choice-content {
    padding: 0 50rpx 20rpx 50rpx;
    box-sizing: border-box;
}
.input-area-item {
    display: flex;
    background:rgba(247,249,252,1);
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 24rpx 30rpx;
    margin-bottom: 20rpx;
}

.input-area-order {
    width: 59rpx;
    height: 56rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #1966FF;
    background:rgba(231,236,241,1);
}

.input-area {
    /* flex: 1; */
    width: 500rpx;
    margin-left: 30rpx;
    padding: 13rpx 0;
    /* display: flex; */
    /* align-items: center; */
    color: #272E40;
    font-family:PingFangSC-Regular,PingFang SC;
    font-size:30rpx;
    word-wrap:break-word
}
.input-area .input__{
    width: 100%;
}

/* 解析部分 */
.analysis-content {
    width: 100%;
    background:rgba(255,255,255,1);
    padding: 20rpx 30rpx 0 30rpx;
    box-sizing: border-box;
    font-size:32rpx;
    position: relative;
    font-family:PingFangSC-Medium,PingFang SC;
    color:rgba(115,134,153,1);
}
.box {
    width: 750rpx;
    height: 20rpx;
    background:rgba(247,249,252,1);
    /* background: red; */
    position: absolute;
    left: 0;
    top: 0rpx;
}
.analysis-content-nav {
    width: 100%;
    height: 120rpx;
    /* line-height: 120rpx; */
    margin: 20rpx 20rpx 0 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size:34rpx;
    font-weight:500;
    color:rgba(244,74,74,1);
}
.analysis-content-nav-right {
    width:112rpx;
    height:44rpx;
    line-height: 44rpx;
    text-align: center;
    background:rgba(255,255,255,0);
    border-radius:22rpx;
    border:1px solid rgba(187,197,205,1);
    font-size:22rpx;
    color:rgba(187,197,205,1);
}
.analysis-content-question {
    width:100%;
    /* height:180rpx; */
    padding: 47rpx 20rpx;
    box-sizing: border-box;
    font-weight:500;
    color:rgba(39,46,64,1);
    border-top: 1px solid #F2F6FA;
}
.analysis-content-question-all {
    margin-top: 28rpx;
    font-size:30rpx;
    color:rgba(10,192,146,1);
}
.analysis-content-question-r {
    margin-left: 120rpx;
}
.analysis-content-question-err {
    color:rgba(244,74,74,1);
}
.analysis-content-question-content {
    margin-top: 28rpx;
    font-size:28rpx;
    color:rgba(115,134,153,1);
    line-height: 1.5;
    letter-spacing: 4rpx;
    font-weight: 400;
}
.parserA view {
    line-height: 1.5!important;
}